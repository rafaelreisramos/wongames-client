{
  "name": "wongames-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest --bail",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "types:check": "tsc --project tsconfig.json --noEmit",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@stripe/react-stripe-js": "^1.4.0",
    "@stripe/stripe-js": "^1.14.0",
    "@styled-icons/boxicons-solid": "^10.23.0",
    "@styled-icons/fa-brands": "^10.26.0",
    "@styled-icons/material-outlined": "^10.28.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "graphql": "^15.5.0",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "11.1.1",
    "next-auth": "^3.19.8",
    "next-pwa": "^5.0.0",
    "next-seo": "^4.24.0",
    "nextjs-progressbar": "^0.0.9",
    "polished": "^4.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.0.10",
    "@types/jest": "^26.0.19",
    "@types/lodash.xor": "^4.5.6",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "apollo": "^2.32.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^11.0.0",
    "msw": "^0.28.2",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "2.3.0",
    "storybook-addon-next-router": "^2.0.4",
    "typescript": "^4.1.3"
  }
}
